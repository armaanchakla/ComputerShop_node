<!DOCTYPE html>
<html>
<head>
	<title>Userlist</title>
</head>
<body>
	
	<center>
		<h1>User List</h1>
		<table border="1">
			<tr>
				<td>ID</td>
				
				<td>USERNAME</td>
				<td>PASSWORD</td>
				<td>ACTION</td>
			</tr>

		<% for(var i=0; i< user.length; i++){ %>
			<tr>
				<td><%= user[i].id %></td>
				<td><%= user[i].username %></td>
				<td><%= user[i].password %></td>
				<td>
					
					<a href="/user/delete/<%= user[i].id %>"> DELETE </a> 
					
				</td>
			</tr>

		<% } %>
		</table>

		<br><br>

		<a href="/home">Back</a> |
		<a href="/logout">logout</a>
	</center>

</body>
</html>